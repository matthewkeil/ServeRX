{
	"compilerOptions": {
		"inlineSourceMap": true,
		"lib": [
			"es2015",
			"es6",
			"es7",
			"dom"
		],
		"module": "commonjs",
		"moduleResolution": "node",
		"noImplicitAny": true,
		"strictNullChecks": true,
		"target": "es2016",
		"typeRoots": [
			"./node_modules/@types"
		]
	},
	"exclude": [
		"./node_modules"
	]
}